# platform :ios, '9.0'

target 'ScreenOn' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for ScreenOn
  pod 'FBSDKLoginKit'
  pod 'GoogleSignIn'
  pod 'Google-Mobile-Ads-SDK'
  pod 'PersonalizedAdConsent'
